doctype html
html(lang='en').maxheight.border
  head
    meta(charset='utf-8')
    title Klasse Grcic: Giacomo Frey - Works
    meta(name='description' content="Giacomo Frey's portfolio")
    meta(name='author' content='Giacomo Frey')
    link(rel='stylesheet' type='text/css' href='header.css')
    link(rel='stylesheet' type='text/css' href='utils.css')
    script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js')
    script(type='text/javascript').
      $.get('header.html',function(response){
      $('#header').html(response);
      });
    script(type='text/javascript').
      $.get('slider.html',function(response){
      $('#slider').html(response);
      });
    body
      #header
      -
        var postimageurl = [
          'https://static.wixstatic.com/media/69d310_316f3f5740ea421b8c3e7a982cbc79d6~mv2.jpg',
          'https://static.wixstatic.com/media/69d310_04991f0f7b1b4bc29d95fd27a306ac10~mv2.jpg',
          'https://static.wixstatic.com/media/69d310_0bc3f22539a648da932b969607e02c93~mv2.jpg',
          'https://static.wixstatic.com/media/69d310_0f90beb3858344f38d8e682116dd364d~mv2.jpg',
          'https://static.wixstatic.com/media/69d310_b38cf6692676472180c3391a3d1390dc~mv2.png',
          'https://static.wixstatic.com/media/69d310_df04472db775483ba84f226300b5e15b~mv2.jpg',
          'https://static.wixstatic.com/media/69d310_acd669a726e84442a9fe74683d611757~mv2.jpg',
          'https://static.wixstatic.com/media/69d310_51ae17eb1ba1485d993b953434ae62fc~mv2.jpg'
