          'https://giacomofrey.de/textura',
          'https://giacomofrey.de/babel-office'
        ]
      .center.flex
        .two-column.inline-block.leftright-4m.bottom-4m
          - for (var x = 0; x < postimageurl.length; x++)
            .hover.inline-block.top-4m.bottom-4m.fullwidth
              a(href=reflinkarray[x])
                img.fullwidth(src=postimageurl[x])
    

                
        



        
